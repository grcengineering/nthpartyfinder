{
  "$schema": "./_schema.json",
  "id": "onelogin",
  "organization": "OneLogin",
  "primary_domain": "onelogin.com",
  "parent_vendor": "one-identity",
  "domains": {
    "onelogin.com": {
      "type": "primary",
      "category": "platform"
    }
  },
  "verification_patterns": [],
  "provider_aliases": [],
  "saas_tenants": [
    {
      "name": "OneLogin",
      "patterns": [
        "{tenant}.onelogin.com"
      ],
      "detection": {
        "success_indicators": [
          "web-login-v2-cdn.onelogin.com/login2",
