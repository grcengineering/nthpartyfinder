{
  "$schema": "./_schema.json",
  "id": "wistia",
  "organization": "Wistia",
  "primary_domain": "wistia.com",
  "domains": {
    "wistia.com": {
      "type": "primary",
      "category": "platform"
    }
  },
  "verification_patterns": [],
  "provider_aliases": [],
  "saas_tenants": [
    {
      "name": "Wistia",
      "patterns": [
        "{tenant}.wistia.com"
      ],
      "detection": {
        "success_indicators": [
          "Sign in to your Wistia account",
          "Wistia",
          "session/new"
        ],
        "failure_indicators": [
          "Page not found"
        ],
        "notes": "Invalid tenants return HTTP 404 with 'Page not found'. Valid tenants redirect to /session/new login page."
      }
    }
  ]
}